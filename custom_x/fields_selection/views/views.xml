<odoo>
    <data>

        <record model="ir.ui.view" id="fields_selection_list">
            <field name="name">fields_selection_form_view</field>
            <field name="model">task_estimation.task_estimation</field>
            <field name="inherit_id" ref="task_estimation.task_estimation_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='comprehension_index']" position="replace">
                    <field name="comprehension_index"/>
                </xpath>

                <xpath expr="//field[@name='technical_risks']" position="replace">
                    <field name="technical_risks"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>